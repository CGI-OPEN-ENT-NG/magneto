{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "title": {
      "type": "string"
    },
    "imageUrl": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "folderId": {
      "type": ["string", "null"]
    },
    "public": {
      "type": "boolean"
    },
    "shared": {
      "type": "boolean"
    },
    "deleted": {
      "type": "boolean"
    },
    "cardIds": {
      "type": "array",
      "items": {
        "anyOf": [
          {
            "type": "string"
          }
        ]
      }
    }
  },
  "required": [
    "title"
  ]
}